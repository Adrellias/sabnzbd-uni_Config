<div class="colmask">
    <div class="section padTable">
        <table id="infoTable">
            <tbody>
                <tr class="alt"><td class="infoTableHeader">SABnzbd $T('version'): </td><td class="infoTableCell">$version</td></tr>
                <tr><td class="infoTableHeader">SABnzbd $T('uptime'): </td><td class="infoTableCell">$uptime</td></tr>
